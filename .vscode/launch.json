{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "winuae",
            "request": "launch",
            "name": "WinUAE Debug",
            "stopOnEntry": false,
            "serverName": "localhost",
            "serverPort": 2345,
            "startEmulator": true,
            "trace": false,
            "exceptionMask": 8188,
            "emulatorStartDelay": 1500,
            "emulator": "${config:amiga-assembly.binDir}/winuae.exe",
            "emulatorWorkingDir": "${config:amiga-assembly.binDir}",
            "program": "${workspaceFolder}/uae/dh0/tinytro",
            "options": [
                "-s",
                "quickstart=a500,1",
                "-s",
                "filesystem=rw,dh0:${workspaceFolder}/uae/dh0",
                "-s",
                "debugging_trigger=SYS:tinytro",
                "-s",
                "kickstart_rom_file=%AmigaRoms%/Kickstart 1.3.rom",
                "-s",
                "debugging_features=gdbserver",
                "-s",
                "sound_output=exact",
                "-s",
                "sound_channels=mono",
                "-s",
                "sound_stereo_separation=0",
                "-s",
                "sound_stereo_mixing_delay=0",
                "-s",
                "sound_max_buff=16384",
                "-s",
                "sound_frequency=48000",
                "-s",
                "sound_interpol=anti",
                "-s",
                "sound_filter=off",
                "-s",
                "sound_filter_type=standard",
                "-s",
                "sound_volume=0",
                "-s",
                "sound_volume_paula=0",
                "-s",
                "sound_volume_cd=0",
                "-s",
                "sound_volume_ahi=0",
                "-s",
                "sound_volume_midi=0",
                "-s",
                "sound_volume_genlock=0",
                "-s",
                "sound_auto=true",
                "-s",
                "sound_max_buff=1024",
                "-s",
                "chipmen_size=2",
                "-s",
                "bogomem_size=4"
            ],
            "preLaunchTask": ""
        },
